<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
	xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/
Framework/View/Layout/etc/page_configuration.xsd">
	<head>
		<css src="Apptha_Marketplace::css/style.css" />
	</head>
	<update handle="customer_account" />
	<body>
		<referenceContainer name="content">
			<block class="Apptha\Marketplace\Block\Product\Manage" name="marketplace_product_manage"
				ifconfig="marketplace/general/enable_in_frontend" template="product/manage.phtml" cacheable="true"/>
				<argument name="cacheable" xsi:type="boolean">true</argument>
		</referenceContainer>
	</body>
</page>